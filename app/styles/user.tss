".win[platform=ios]": {
	left:'320dp',
	width:'320dp'
},
".win[platform=android]": {
	fullscreen:true,
	navBarHidden:true
},
'.infoHeader': {
	backgroundColor:'#8557658D',
	height:Ti.UI.SIZE,
	bottom:0,
	layout:'vertical',
	zIndex:100
},
'.infoSteps': {
	backgroundColor:'#57658D',
	top:Ti.Platform.displayCaps.platformHeight,
	layout:'vertical'
},
'Label': {
	color:'white',
	font:{fontFamily:'Helvetica Neue', fontSize:'16dp'},
	left:'10dp',
	right:'10dp'
},
'.step': {
	height:'70dp',
	borderColor:'#CCC',
	borderWidth:1
},
'#scrollview': {
	contentHeight:'auto'
},
'#avatar': {
	defaultImage:'none',
	top:0,
	height:Ti.Platform.displayCaps.platformHeight,
	width:Ti.Platform.displayCaps.platformWidth
},
'.right': {
	textAlign:'right'
},
'.top': {
	top:'10dp'
},
'#talkmeabout': {
	top:'40dp',
	textAlign:'right'
},
'.talkmeabout': {
	height:Ti.UI.SIZE
},
'#messageSender': {
	backgroundColor:'#57658D',
	bottom:'25dp'
},
'#messageSenderArea': {
	height:'250dp',
	top:'-210dp',
	zIndex:100
},
'#send': {
	bottom:'12dp',
	height:'35dp',
	width:'70dp',
	backgroundImage:'none',
	backgroundColor:'#57658D',
	borderColor:'#CCC',
	borderWidth:1
},
'#sendImage': {
	image:'/images/send.png',
	width:'32dp'
},
'#textarea': {
	left:'10dp',
	right:'10dp',
	top:'10dp',
	bottom:'10dp',
	enabled:false,
	font:{fontSize:'18px'},
	returnKeyType:Ti.UI.RETURNKEY_SEND
},
'#prevMsg': {
	opacity:0
},
'#messages': {
	zIndex:50,
	opacity:0,
	bottom:'330dp',
	backgroundColor:'#9057658D',
	separatorColor:'transparent'
},
'#loader':{
	color:'#CCC',
	font:{fontSize:'13px'}
}
